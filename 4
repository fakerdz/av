repeat
    wait()
until game:IsLoaded() 
local LocalizationService = game:GetService("LocalizationService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local originalFunction = LocalizationService.GetCountryRegionForPlayerAsync

hookfunction(originalFunction, function(self)
    return "US"
end)

local result, code = pcall(originalFunction, LocalizationService, player)
print("After Hooking:", code, result)
