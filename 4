local Workspace = game:GetService("Workspace")
local keyword = "Chest"  -- Từ khóa cần tìm trong tên của đối tượng

-- Hàm đệ quy để tìm các đối tượng có chứa từ khóa trong tên và in ra thông tin chi tiết
local function searchForKeywordInChildren(parent)
    for _, object in pairs(parent:GetChildren()) do
        if string.find(object.Name, keyword) then
            print("Đối tượng chứa từ khóa '" .. keyword .. "':", object.Name)

            -- In ra các thuộc tính của đối tượng
            print("Các thuộc tính của đối tượng:")
            for key, value in pairs(object:GetAttributes()) do
                print(" -", key, value)
            end
            
            -- In ra thông tin về các phần tử con và thử lấy thêm thông tin mã
            print("Thông tin chi tiết của các đối tượng con:")
            for _, child in pairs(object:GetChildren()) do
                print(" -- Tên:", child.Name, "Loại:", child.ClassName)
                
                -- In ra tất cả các thuộc tính của đối tượng con
                for attribute, value in pairs(child:GetAttributes()) do
                    print(" ---", attribute, value)
                end
                
                -- Thử in giá trị nếu đối tượng có thể có một tham chiếu hoặc mã đặc biệt
                if child:IsA("Model") or child:IsA("Folder") then
                    for subkey, subvalue in pairs(child:GetChildren()) do
                        print(" --- Phần tử bên trong:", subvalue.Name, "Loại:", subvalue.ClassName)
                        for subattr, subval in pairs(subvalue:GetAttributes()) do
                            print(" ---- Thuộc tính:", subattr, subval)
                        end
                    end
                end
            end
        end
        -- Tiếp tục tìm kiếm trong các đối tượng con nếu có
        if #object:GetChildren() > 0 then
            searchForKeywordInChildren(object)
        end
    end
end

-- Bắt đầu tìm từ Workspace
searchForKeywordInChildren(Workspace)
