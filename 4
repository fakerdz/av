local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local Player = Players.LocalPlayer

-- Bước 1: Nhấp vào ClickDetector của Crystal Chest
local chest = Workspace:FindFirstChild("CrystalChest")  -- Đảm bảo tên đúng
local clickDetector = chest and chest:FindFirstChildOfClass("ClickDetector")

if clickDetector then
    -- Nhấp vào Crystal Chest để mở bảng tùy chọn
    fireclickdetector(clickDetector)
    print("Đã nhấp vào Crystal Chest để mở bảng tùy chọn.")
    
    -- Bước 2: Đợi bảng tùy chọn hiện ra, sau đó nhấp vào "Unlock 10"
    wait(1)  -- Đợi một chút để bảng hiện ra

    -- Tìm GUI của bảng và nhấp vào nút "Unlock 10"
    local gui = Player.PlayerGui:FindFirstChild("TênGiaoDiện")  -- Đặt tên GUI chính xác nếu có

    if gui then
        local unlock10Button = gui:FindFirstChild("Unlock10Button")  -- Đảm bảo tên nút đúng
        
        if unlock10Button then
            -- Giả lập nhấp vào nút "Unlock 10"
            unlock10Button:FireClick()  -- Giả sử nút có phương thức `FireClick`, có thể cần điều chỉnh
            print("Đã chọn Unlock 10.")
        else
            print("Không tìm thấy nút Unlock 10 trong GUI.")
        end
    else
        print("Không tìm thấy GUI của bảng tùy chọn.")
    end
else
    print("Không tìm thấy Crystal Chest hoặc ClickDetector.")
end
